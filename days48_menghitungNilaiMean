public class days48 {
    public static void main(String[] args) {
        
      int [] nilai = {9,8,4,7,6}; //deklarasi array integer
        
        //menentukan jumlah elemen array
        int jumlah = nilai.length;
        
        //menentukan totl nilai array
        int total = 0;
        for(int i = 0; i < jumlah ; i++){
            total = nilai [i];
        }
        //menentukan nilai mean
        double mean = total /jumlah;
        
        System.out.println("nilai mean = "+mean);
       
    }
}
